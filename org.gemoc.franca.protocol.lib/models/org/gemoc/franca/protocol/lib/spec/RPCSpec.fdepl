specification org.gemoc.franca.protocol.lib.spec.RPCSpec {
	for methods {
		CallSemantics: {synchronous, asynchronous} (default: asynchronous);
		IsOptional: Boolean (default: true);
		IsNotification : Boolean (default: true);
	}
	for interfaces {
		Opposite: Interface;
		IsClient: Boolean;
	}
	for attributes {
		IsOptional: Boolean (default:true);
	}
	
}