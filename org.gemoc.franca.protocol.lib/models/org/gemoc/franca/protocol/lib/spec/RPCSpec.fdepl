package org.gemoc.franca.protocol.lib.spec

specification RPCSpec {
	for methods {
		CallType: {notification, request} (default: request);
		IsOptional: Boolean (default: true);
	}
	for interfaces {
		Opposite: Interface;
		IsClient: Boolean;
	}
	for attributes {
		IsOptional: Boolean (default:true);
	}
	
}